{
  "name": "pullap-ai-service",
  "version": "1.0.0",
  "description": "Get Training Data for Pullap AI",
  "keywords": [
    "pullap",
    "ai",
    "service",
    "training",
    "data"
  ],
  "main": "src/index.ts",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:antonkalik/pullap-service.git"
  },
  "scripts": {
    "start": "NODE_ENV=production node dist/bundle.js",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "dev": "cross-env NODE_ENV=development && npm-run-all -p dev:*",
    "dev:build": "webpack --config webpack.config.js --watch",
    "dev:start": "nodemon --watch dist --exec node dist/bundle.js",
    "test": "NODE_ENV=test jest --config ./jest.config.js",
    "lint": "eslint ./src -c .eslintrc.json",
    "db:migrate:make": "npx knex --knexfile ./src/database/knexfile.ts migrate:make $1 -x ts",
    "db:migrate:latest": "npx knex --knexfile ./src/database/knexfile.ts migrate:latest",
    "db:seed": "npx knex --knexfile ./src/database/knexfile.ts seed:run",
    "db:seed:file": "npx knex --knexfile ./src/database/knexfile.ts seed:run --specific $1",
    "db:rollback:all": "npx knex --knexfile ./src/database/knexfile.ts migrate:rollback --all",
    "db:rollback": "npx knex --knexfile ./src/database/knexfile.ts migrate:rollback"
  },
  "author": "anton kalik",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "knex": "^2.5.1",
    "openai": "^3.3.0",
    "pg": "^8.11.1",
    "redis": "^4.6.7",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/eslint-parser": "^7.22.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "@faker-js/faker": "^8.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/faker": "^6.6.9",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.4.2",
    "@types/redis": "^4.0.11",
    "@types/validator": "^13.7.17",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-jest": "^29.6.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^5.1.6",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "engines": {
    "node": "18.13.x"
  }
}
