{
  "name": "pullap-ai-service",
  "version": "1.0.0",
  "description": "Get Training Data for Pullap AI",
  "keywords": [
    "pullap",
    "ai",
    "service",
    "training",
    "data"
  ],
  "main": "src/index.ts",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:antonkalik/pullap-service.git"
  },
  "scripts": {
    "start": "NODE_ENV=production node dist/bundle.js",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "dev": "cross-env NODE_ENV=development && npm-run-all -p dev:*",
    "dev:build": "webpack --config webpack.config.js --watch",
    "dev:start": "nodemon --watch dist --exec node dist/bundle.js",
    "test": "NODE_ENV=test jest --config ./jest.config.js",
    "lint": "eslint ./src -c .eslintrc.json",
    "knex:make": "npx knex --knexfile ./src/database/knexfile.ts migrate:make $1 -x ts",
    "knex:migrate": "npx knex --knexfile ./src/database/knexfile.ts migrate:latest",
    "knex:seed": "npx knex --knexfile ./src/database/knexfile.ts seed:run",
    "knex:reset": "yarn db:migrate && npm run db:seed"
  },
  "author": "anton kalik",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "knex": "^2.4.2",
    "openai": "^3.2.1",
    "pg": "^8.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@faker-js/faker": "^8.0.0",
    "@types/faker": "^6.6.9",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "babel-jest": "^29.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.40.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^5.0.4",
    "webpack": "^5.82.1",
    "webpack-cli": "^5.1.1",
    "webpack-node-externals": "^3.0.0"
  },
  "engines": {
    "node": "18.13.x"
  }
}
